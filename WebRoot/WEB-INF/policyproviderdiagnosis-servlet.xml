<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

	
	<bean id="policyProviderDiagnosisValidator" class="com.ametis.cms.web.validator.PolicyProviderDiagnosisValidator"/>

	<bean id="urlMapping" class="org.springframework.web.servlet.handler.SimpleUrlHandlerMapping">
		<property name="mappings">
			<props>
				<prop key="/policyproviderdiagnosis">policyProviderDiagnosisController</prop>
				<prop key="/policyproviderdiagnosis-form">policyProviderDiagnosisFormController</prop>
			</props>
		</property>
	</bean>
	<bean id="viewResolver" class="org.springframework.web.servlet.view.InternalResourceViewResolver">
		<property name="viewClass">
			<value>org.springframework.web.servlet.view.JstlView</value>
		</property>
		<property name="prefix">
			<value>WEB-INF/jsp/policyProviderDiagnosis/</value>
		</property>
		<property name="suffix">
			<value>.jsp</value>
		</property>
	</bean>
	<bean name="policyProviderDiagnosisController" class="com.ametis.cms.web.controller.PolicyProviderDiagnosisController">
		<property name="policyProviderDiagnosisService">
			<ref bean="policyProviderDiagnosisService" />
		</property>
		<property name="countSet">
			<ref bean="countSet" />
		</property>
		<property name="maxPercountSet">
			<ref bean="maxPercountSet" />
		</property>
		<property name="alertProperties">
			<ref bean="alertProperties" />
		</property>
		<property name="securityService">
			<ref bean="securityService" />
		</property>

				<property name="userService">
			<ref bean="userService" />
		</property>
		
	</bean>

	<bean name="policyProviderDiagnosisFormController" class="com.ametis.cms.web.controller.PolicyProviderDiagnosisFormController">
		<property name="formView" value="editPolicyProviderDiagnosis"/>
		<property name="successView" value="ownerRedirect"/>
		<property name="validator" ref="policyProviderDiagnosisValidator"/>
		<property name="commandName" value="policyProviderDiagnosisForm" />
		<property name="commandClass" value="com.ametis.cms.web.form.PolicyProviderDiagnosisForm" />

		<property name="policyProviderDiagnosisService">
			<ref bean="policyProviderDiagnosisService" />
		</property>
		<property name="securityService" >
			<ref bean="securityService" />
		</property>

				<property name="userService">
			<ref bean="userService" />
		</property>
		


		<property name="propertiesUtil">
			<ref bean="alertProperties" />
		</property>

<!-- foreign affairs -->
	
			<property name="diagnosisService">
			<ref bean="diagnosisService" />
		</property>
				<property name="policyProviderService">
			<ref bean="policyProviderService" />
		</property>
			
<!-- foreign affairs end -->

	</bean>
	<bean id="alertProperties" class="org.springframework.context.support.ResourceBundleMessageSource">
		<property name="basename"><value>com.ametis.cms.messages.policyProviderDiagnosis</value></property>
	</bean>
</beans>


