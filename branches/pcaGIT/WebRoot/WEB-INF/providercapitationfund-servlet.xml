<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

	
	<bean id="providerCapitationFundValidator" class="com.ametis.cms.web.validator.ProviderCapitationFundValidator"/>

	<bean id="urlMapping" class="org.springframework.web.servlet.handler.SimpleUrlHandlerMapping">
		<property name="mappings">
			<props>
				<prop key="/providercapitationfund">providerCapitationFundController</prop>
				<prop key="/providercapitationfund-form">providerCapitationFundFormController</prop>
			</props>
		</property>
	</bean>
	<bean id="viewResolver" class="org.springframework.web.servlet.view.InternalResourceViewResolver">
		<property name="viewClass">
			<value>org.springframework.web.servlet.view.JstlView</value>
		</property>
		<property name="prefix">
			<value>WEB-INF/jsp/providerCapitationFund/</value>
		</property>
		<property name="suffix">
			<value>.jsp</value>
		</property>
	</bean>
	<bean name="providerCapitationFundController" class="com.ametis.cms.web.controller.ProviderCapitationFundController">
		<property name="providerCapitationFundService">
			<ref bean="providerCapitationFundService" />
		</property>
		<property name="countSet">
			<ref bean="countSet" />
		</property>
		<property name="maxPercountSet">
			<ref bean="maxPercountSet" />
		</property>
		<property name="alertProperties">
			<ref bean="alertProperties" />
		</property>
		<property name="securityService">
			<ref bean="securityService" />
		</property>

				<property name="userService">
			<ref bean="userService" />
		</property>
		
	</bean>

	<bean name="providerCapitationFundFormController" class="com.ametis.cms.web.controller.ProviderCapitationFundFormController">
		<property name="formView" value="editProviderCapitationFund"/>
		<property name="successView" value="ownerRedirect"/>
		<property name="validator" ref="providerCapitationFundValidator"/>
		<property name="commandName" value="providerCapitationFundForm" />
		<property name="commandClass" value="com.ametis.cms.web.form.ProviderCapitationFundForm" />

		<property name="providerCapitationFundService">
			<ref bean="providerCapitationFundService" />
		</property>
		<property name="securityService" >
			<ref bean="securityService" />
		</property>

				<property name="userService">
			<ref bean="userService" />
		</property>
		


		<property name="propertiesUtil">
			<ref bean="alertProperties" />
		</property>

<!-- foreign affairs -->
	
		
<!-- foreign affairs end -->

	</bean>
	<bean id="alertProperties" class="org.springframework.context.support.ResourceBundleMessageSource">
		<property name="basename"><value>com.ametis.cms.messages.providerCapitationFund</value></property>
	</bean>
</beans>


