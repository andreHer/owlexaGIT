<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

	
	<bean id="groupBenefitValidator" class="com.ametis.cms.web.validator.GroupBenefitValidator"/>

	<bean id="urlMapping" class="org.springframework.web.servlet.handler.SimpleUrlHandlerMapping">
		<property name="mappings">
			<props>
				<prop key="/groupbenefit">groupBenefitController</prop>
				<prop key="/groupbenefit-form">groupBenefitFormController</prop>
			</props>
		</property>
	</bean>
	<bean id="viewResolver" class="org.springframework.web.servlet.view.InternalResourceViewResolver">
		<property name="viewClass">
			<value>org.springframework.web.servlet.view.JstlView</value>
		</property>
		<property name="prefix">
			<value>WEB-INF/jsp/groupBenefit/</value>
		</property>
		<property name="suffix">
			<value>.jsp</value>
		</property>
	</bean>
	<bean name="groupBenefitController" class="com.ametis.cms.web.controller.GroupBenefitController">
		<property name="groupBenefitService">
			<ref bean="groupBenefitService" />
		</property>
		<property name="countSet">
			<ref bean="countSet" />
		</property>
		<property name="maxPercountSet">
			<ref bean="maxPercountSet" />
		</property>
		<property name="alertProperties">
			<ref bean="alertProperties" />
		</property>
<property name="securityService">
			<ref bean="securityService" />
		</property>
				<property name="userService">
			<ref bean="userService" />
		</property>
		<property name="logService">
            <ref bean="activityLogService" />
        </property>
	</bean>

	<bean name="groupBenefitFormController" class="com.ametis.cms.web.controller.GroupBenefitFormController">
		<property name="formView" value="editGroupBenefit"/>
		<property name="successView" value="ownerRedirect"/>
		<property name="validator" ref="groupBenefitValidator"/>
		<property name="commandName" value="groupBenefitForm" />
		<property name="commandClass" value="com.ametis.cms.web.form.GroupBenefitForm" />

		<property name="groupBenefitService">
			<ref bean="groupBenefitService" />
		</property>
<property name="logService">
            <ref bean="activityLogService" />
        </property>

				<property name="userService">
			<ref bean="userService" />
		</property>
		
<property name="securityService">
			<ref bean="securityService" />
		</property>

		<property name="propertiesUtil">
			<ref bean="alertProperties" />
		</property>

<!-- foreign affairs -->
	
			<property name="memberGroupService">
			<ref bean="memberGroupService" />
		</property>
				<property name="itemCategoryService">
			<ref bean="itemCategoryService" />
		</property>
			
<!-- foreign affairs end -->

	</bean>
	<bean id="alertProperties" class="org.springframework.context.support.ResourceBundleMessageSource">
		<property name="basename"><value>com.ametis.cms.messages.groupBenefit</value></property>
	</bean>
</beans>


